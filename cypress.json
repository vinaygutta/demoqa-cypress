{
    "pageLoadTimeout": 120000,
    "env": {
        "platform": "dev"
    },
    "video": false,
    "reporter": "cypress-multi-reporters",
    "reporterOptions": {
        "reporterEnabled": "mochawesome",
        "mochawesomeReporterOptions": {
            "quite": true,
            "overwrite": false,
            "html": false,
            "json": true
        }
    },
    "screenshotOnRunFailure": true,
    "screenshotsFolder": "./mochawesome-report/HTMLReport/screenshots"
}